<h1 id="title">Tags</h1>

<ul>
  <% @tags.each do |tag| %>
    <li class="tag_entry"><span id="tag_title"><%= link_to tag.name, tag_path(tag) %></span><%= link_to 'Delete Tag', tag_path(tag), method: :delete, data: {confirm: "Are you sure you want to delete the tag?"}, id: 'delete_tag' %></li>
  <% end %>
</ul>

<%= link_to 'Articles Index', articles_path, id: "return_to_articles_tag" %>
